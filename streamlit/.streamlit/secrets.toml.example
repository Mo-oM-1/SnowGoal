# Copy this file to secrets.toml and fill in your Snowflake credentials
# For Streamlit Cloud: add these in the app settings > Secrets

[connections.snowflake]
account = "your-account.region"
user = "your-username"
password = "your-password"
warehouse = "SNOWGOAL_WH_XS"
database = "SNOWGOAL_DB"
schema = "GOLD"
role = "SNOWGOAL_ROLE"
